{
  "key": 2,
  "title": "Extract a field from a JSON object",
  "description": "Extract a field from a JSON object using `jq`",
  "tags": ["json", "jq", "file manipulation"],
  "arguments": [
    {
      "name": "File name",
      "placeholder": "channel_versions",
      "id": "FILE_NAME"
    },
    { "name": "Field", "placeholder": "dev.version", "id": "FIELD" }
  ],
  "command": "cat $FILE_NAME.json | jq '.$FIELD'"
}
